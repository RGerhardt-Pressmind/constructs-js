{
  "name": "constructs-js",
  "version": "1.1.1",
  "description": "A NodeJS based data generator. This returns an infinite amount of randomly generated data according to a structure or inserts it into a database system.",
  "contributors": [
    "Robbyn Gerhardt <r.gerhardt@team-braeutigam.de>"
  ],
  "bugs": "https://github.com/RGerhardt-Pressmind/constructs-js/issues",
  "homepage": "https://rgerhardt-pressmind.github.io/constructs-js/",
  "repository": {
    "type": "git",
    "url": "git://git@github.com:RGerhardt-Pressmind/constructs-js.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "node main.js",
    "build": "browserify index.js -s constructs -o dist/constructs.js && minify dist/constructs.js > dist/constructs.min.js"
  },
  "publishConfig": {
    "registry":"https://registry.npmjs.org"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "generator",
    "random",
    "template",
    "json",
    "angular",
    "typescript",
    "node",
    "browser",
    "unique",
    "angular2",
    "angular4",
    "random generator",
    "special data",
    "locales",
    "multilanguage data",
    "large data generator"
  ],
  "license": "MIT",
  "devDependencies": {
    "browserify": "^17.0.0",
    "minify": "^8.0.3"
  }
}
